   "source": [
    "function uniteUnique(...arr) {\n",
    "  let newarr = [];\n",
    "  for (let array of arr) {\n",
    "    for (let value of array) {\n",
    "      if (!newarr.includes(value)){\n",
    "        newarr.push(value);\n",
    "      }\n",
    "    }\n",
    "  }\n",
    "  return newarr;\n",
    "}\n",
    "\n",
    "console.log(uniteUnique([1, 3, 2], [5, 2, 1, 4], [2, 1]));"
   ]
  }
