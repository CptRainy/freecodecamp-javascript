   "source": [
    "function sumAll(arr) {\n",
    "  let sum = 0;\n",
    "  let big;\n",
    "  let small;\n",
    "\n",
    "  if(arr[0] > arr[1]){\n",
    "    big = arr[0];\n",
    "    small = arr[1];\n",
    "  } else{\n",
    "    big = arr[1];\n",
    "    small = arr[0];\n",
    "  }\n",
    "  \n",
    "  for(let i = small; i <= big; i++ ){\n",
    "    sum += i;\n",
    "  }\n",
    "  return sum;\n",
    "}\n",
    "\n",
    "sumAll([1, 4]);"
   ]
  }
