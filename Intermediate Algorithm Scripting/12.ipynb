   "source": [
    "function sumFibs(num) {\n",
    "  let prev = 0;\n",
    "  let curr = 1;\n",
    "  let result = 0;\n",
    "  while(curr <= num){\n",
    "    if(curr % 2 !== 0){\n",
    "      result += curr;\n",
    "    }\n",
    "    let temp = curr;\n",
    "    curr += prev;\n",
    "    prev = temp;\n",
    "  }\n",
    "  return result;\n",
    "}\n",
    "\n",
    "console.log(sumFibs(4));"
   ]
  }
