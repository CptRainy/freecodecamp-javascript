    "function chunkArrayInGroups(arr, size) {\n",
    "  let arr2 = [];\n",
    "  for (let i = 0; i < arr.length; i+=size){  \n",
    "    arr2.push(arr.slice(i,i+size));\n",
    "  }\n",
    "\n",
    "  return arr2;\n",
    "}\n",
    "\n",
    "chunkArrayInGroups([\"a\", \"b\", \"c\", \"d\"], 2);"
   ]
  }
